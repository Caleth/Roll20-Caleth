         <RulesElement name="Healing Infusion (Hybrid)" type="Class Feature" internal-id="ID_FMP_CLASS_FEATURE_2159" charelem="151" legality="rules-legal" >
            <specific name="Level" > 1 </specific>
            <specific name="Powers" > ID_FMP_POWER_4128,ID_FMP_POWER_7635,ID_FMP_POWER_10187 </specific>
