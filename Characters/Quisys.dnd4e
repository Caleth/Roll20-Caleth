         <RulesElement name="Telepathy 5" type="Racial Trait" internal-id="ID_FMP_RACIAL_TRAIT_1813" charelem="18" legality="rules-legal" >
            <specific name="Short Description" > Two way telepathic communication within 5. </specific>
            <specific name="Level" > 1 </specific>
            <specific name="Class" > ID_FMP_RACE_46 </specific>
